{
	"name": "emmylua-codestyle",
	"displayName": "EmmyLuaCodeStyle",
	"description": "format lua code and check lua code style",
	"keywords": [
		"lua",
		"emmylua",
		"format",
		"codestyle",
		"luaformat",
		"lua formatter",
		"lua codestyle"
	],
	"version": "0.2.4",
	"author": "CppCXY",
	"publisher": "CppCXY",
	"engines": {
		"vscode": "^1.62.0"
	},
	"repository": "https://github.com/CppCXY/EmmyLuaCodeStyle",
	"categories": [
		"Programming Languages",
		"Formatters",
		"Linters"
	],
	"activationEvents": [
		"onLanguage:lua",
		"onCommand:emmyluacodestyle.insertEditorConfig"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emmyluacodestyle.insertEditorConfig",
				"title": "insert luaCodeStyle template"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"download": "^8.0.0"
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	}
}
